    </preferences>

    <query>
        <select>cliente.codigo as "Código do Cliente", cliente.nome as "Nome do Cliente", cliente.telefone as "Telefone", cliente.endereco as "Endereço", cliente.idade as "Idade do Cliente", cliente.foto as "Foto do Cliente", cliente.ref_cidade as "Cidade do Cliente", cidade.descricao as "Descrição da Cidade", estado.codigo as "Código do Estado", estado.descricao as "Descricao do Estado"</select>
        <from>cliente,cidade,estado</from>
        <where>cidade.ref_estado = estado.codigo and cliente.ref_cidade = cidade.codigo</where>
        <groupby></groupby>
        <orderby>cliente.nome asc</orderby>
    </query>

