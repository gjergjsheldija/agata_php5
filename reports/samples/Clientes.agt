<?xml version="1.0"?>
<report>
    <description>Lista de Clientes, carta e etiquetas</description>
    <datasource>
        <name>Exemplos</name>
    </datasource>

    <header>
        <body></body>
        <align>center</align>
    </header>

    <footer>
        <body></body>
        <align>center</align>
