    <preferences>
        <showgroup></showgroup>
        <showdetail>1</showdetail>
        <showlabel></showlabel>
        <shownumber>1</shownumber>
        <showindent>1</showindent>
        <distinct></distinct>
        <pagesetup>
            <format>A3</format>
            <orientation>landscape</orientation>
            <marginleft>0</marginleft>
